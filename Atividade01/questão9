numero1 = int(input("Insira um número: "))
numero2 = int(input("Insira um número: "))
numero3 = int(input("Insira um número: "))

max = numero1

if numero2 > max:
    max = numero2

elif numero3 > max:
    max = numero3

min = numero1

if numero2< min:
    min = numero2

elif numero3 < min:
    min = numero3

print("O maior número será: ", max)

print("O menor número será: ", min)
